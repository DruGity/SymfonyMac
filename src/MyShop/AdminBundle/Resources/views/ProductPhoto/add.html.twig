{% extends "@MyShopAdmin/layout.html.twig" %}

{% block body %}
	<div class="progressbar-heading grids-heading">
		<h4>Добавление фото к товару {{ product.model }}</h4>				
	</div>
    <form enctype="multipart/form-data"{#для заливки файла на сервер#} method="post" action="{{ path("my_shop_admin.product_photo_add", {idProduct:product.getId()}) }}">
        {{ form_widget(form) }}
        <input type="submit" value="Добавить" />
    </form>
{% endblock %}